<div class="modal" id="<%= modalid %>">
	<div class="modal-dialog">
      	<div class="modal-content">
        	<div class="modal-header">
          		<button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
          		<h4 class="modal-title"><%= home %> Time Outs</h4>
        	</div>
        	<div class="modal-body">
        		<%= form_tag lacrosstimeout_sport_team_gameschedule_path(@sport, @team, @gameschedule), class: "form-horizontal", method: 'put' do %>
        			<%= hidden_field_tag "home", home %>
        			<div class="row">
        				<div class="col-xs-1">
        					<h5>Period</h5>
        				</div>
        				<div class="col-xs-4 col-xs-offset-1">
        					<h5>1st</h5>
        				</div>
        				<div class="col-xs-4">
        					<h5>2nd</h5>
        				</div>
        			</div>
        			<div class="row">
                        <% timeout1 = firsttimeouts[0] ? firsttimeouts[0].split(':') : "" %>
                        <% timeout2 = secondtimeouts[0] ? secondtimeouts[0].split(':') : "" %>
        				<div class="col-xs-1">
        					<h5>1</h5>
        				</div>
        				<div class="col-xs-2 col-xs-offset-1">
                            <%= label_tag 'Minutes' %>
                            <%= select_tag 'firsttimeout_minutes1', options_for_select(textminutes(15), timeout1[0]), { class: 'form-control' } %>
                        </div>
                        <div class="col-xs-2">
                            <%= label_tag 'Seconds' %>
                            <%= select_tag 'firsttimeout_seconds1', options_for_select(textseconds, timeout1[1]), { class: 'form-control' } %>
        				</div>
        				<div class="col-xs-2">
                            <%= label_tag 'Minutes' %>
                            <%= select_tag 'secondtimeout_minutes1', options_for_select(textminutes(15), timeout2[0]), { class: 'form-control' } %>
                        </div>
                        <div class="col-xs-2">
                            <%= label_tag 'Seconds' %>
                            <%= select_tag 'secondtimeout_seconds1', options_for_select(textseconds, timeout2[1]), { class: 'form-control' } %>
        				</div>
        			</div>
        			<p>
         			<div class="row">
                        <% timeout1 = firsttimeouts[1] ? firsttimeouts[1].split(':') : "" %>
                        <% timeout2 = secondtimeouts[1] ? secondtimeouts[1].split(':') : "" %>
        				<div class="col-xs-1">
        					<h5>2</h5>
        				</div>
                        <div class="col-xs-2 col-xs-offset-1">
                            <%= label_tag 'Minutes' %>
                            <%= select_tag 'firsttimeout_minutes2', options_for_select(textminutes(15), timeout1[0]), { class: 'form-control' } %>
                        </div>
                        <div class="col-xs-2">
                            <%= label_tag 'Seconds' %>
                            <%= select_tag 'firsttimeout_seconds2', options_for_select(textseconds, timeout1[1]), { class: 'form-control' } %>
                        </div>
                        <div class="col-xs-2">
                            <%= label_tag 'Minutes' %>
                            <%= select_tag 'secondtimeout_minutes2', options_for_select(textminutes(15), timeout2[0]), { class: 'form-control' } %>
                        </div>
                        <div class="col-xs-2">
                            <%= label_tag 'Seconds' %>
                            <%= select_tag 'secondtimeout_seconds2', options_for_select(textseconds, timeout2[1]), { class: 'form-control' } %>
                        </div>
        			</div>
        			<p>
         			<div class="row">
                        <% timeout1 = firsttimeouts[2] ? firsttimeouts[2].split(':') : "" %>
                        <% timeout2 = secondtimeouts[2] ? secondtimeouts[2].split(':') : "" %>
                        <div class="col-xs-1">
                            <h5>3</h5>
                        </div>
                        <div class="col-xs-2 col-xs-offset-1">
                            <%= label_tag 'Minutes' %>
                            <%= select_tag 'firsttimeout_minutes3', options_for_select(textminutes(15), timeout1[0]), { class: 'form-control' } %>
                        </div>
                        <div class="col-xs-2">
                            <%= label_tag 'Seconds' %>
                            <%= select_tag 'firsttimeout_seconds3', options_for_select(textseconds, timeout1[1]), { class: 'form-control' } %>
                        </div>
                        <div class="col-xs-2">
                            <%= label_tag 'Minutes' %>
                            <%= select_tag 'secondtimeout_minutes3', options_for_select(textminutes(15), timeout2[0]), { class: 'form-control' } %>
                        </div>
                        <div class="col-xs-2">
                            <%= label_tag 'Seconds' %>
                            <%= select_tag 'secondtimeout_seconds3', options_for_select(textseconds, timeout2[1]), { class: 'form-control' } %>
                        </div>
        			</div>
        			<p>
       				<div class="row">
        				<div class="col-xs-2 col-xs-offset-4">
        					<%= submit_tag :submit, class: 'btn btn-primary' %>
        				</div>
        			</div>
        		<% end %>
         	</div>
        	<div class="modal-footer">
          		<a href="#" data-dismiss="modal" class="btn btn-default">Close</a>
          		<!--%= link_to "Ok", lacrosstimeout_sport_team_gameschedule_path(@sport, @team, @gameschedule, home: home), :class => "btn btn-primary" %-->
        	</div>
        </div>
	</div> 
</div>