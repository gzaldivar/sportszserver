<script>
  $(document).ready(function() {
    $('.coachselected').click(function() {
      $.ajax({
        url: "<%= sport_coaches_path(current_site, team_id: current_team.id) %>",
        dataType: "script"
      });
    });
   $('.eventselected').click(function() {
      $.ajax({
        url: "<%= selectteam_sport_path(current_site, team_id: current_team.id) %>",
        dataType: "script"
      });
    });
  });
</script>
